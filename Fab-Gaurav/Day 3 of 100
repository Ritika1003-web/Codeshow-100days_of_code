CodeChef  Problem Code:-  CHOPRT

#include <iostream>
using namespace std;
int main() {
int t,a,b;
cin>>t;
for(int i=0;i<t;i++)
    {cin>>a>>b;
        {
            if(a>b)
                cout<<">\n";
            else if(a<b)
                cout<<"<\n";
            else
                cout<<"=\n";
        }
    }
	return 0;
}






CodeChef  Problem Code:-  SNCKYEAR

#include <iostream>
using namespace std;
int main() {
int t,year;
cin>>t;
for(int i=0;i<t;i++)
    {cin>>year;
       { if(year==2010||year==2015||year==2016||year==2017||year==2019)
            cout<<"HOSTED\n";
        else
            cout<<"NOT HOSTED\n";
        }
    }
return 0;
}






CodeChef  Problem Code:-  FLOW008

#include <iostream>
using namespace std;

int main() {
int num,n,t;
cin>>n;
for(int i=0;i<n;i++)
 { cin>>num;
	{
	    if(num<10)
	     cout<<"Thanks for helping Chef!\n";
	    else
	     cout<<"-1\n";
	}
 }
	return 0;
}
