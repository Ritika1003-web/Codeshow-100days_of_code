#include <iostream>
using namespace std;

int main() 
{
	int t,a,b,s=0;
	cin>>t;
	for(int i=0; i<t; i++)
	{
	    s=0;
	    cin>>a>>b;
	    int x=a+b;
	    int y;
	    while(x!=0)
	    {
	        y=x%10;
	        x=x/10;
	        switch(y)
	        {
	            case 0:
	            {
	                s=s+6;
	                break;
	            }
	            case 1:
	            {
	                s=s+2;
	                break;
	            }
	            case 2:
	            {
	                s=s+5;
	                break;
	            }
	            case 3:
	            {
	                s=s+5;
	                break;
	            }
	            case 4:
	            {
	                s=s+4;
	                break;
	            }
	            case 5:
	            {
	                s=s+5;
	                break;
	            }
	            case 6:
	            {
	                s=s+6;
	                break;
	            }
	            case 7:
	            {
	                s=s+3;
	                break;
	            }
	            case 8:
	            {
	                s=s+7;
	                break;
	            }
	            case 9:
	            {
	                s=s+6;
	                break;
	            }
	        }
	    }
	    cout<<s<<endl;
	}
	return 0;
}
