#include <iostream>
using namespace std;

int main() 
{
    int n,d,l;
    cin>>n;
    while(n!=0)
    {
        d=n%10;
        n=n/10;
        l++;
    }
    if(l==1)
    {
        cout<<"1"<<endl;
    }
    if(l==2)
    {
        cout<<"2"<<endl;
    }
    if(l==3)
    {
        cout<<"3"<<endl;
    }
    if(l>3)
    {
        cout<<"More than 3 digits";
    }
	// your code goes here
	return 0;
}
